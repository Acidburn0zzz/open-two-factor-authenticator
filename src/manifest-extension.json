{
  "name": "Open Two-Factor Authenticator",
  "description": "A sync-able open-source two-factor authenticator",
  "author": "InBasic",
  "version": "0.1.1",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "notifications",
    "clipboardWrite",
    "clipboardRead"
  ],
  "browser_action": {
    "default_icon": {
      "19": "data/icons/19.png",
      "38": "data/icons/38.png"
    },
    "default_popup": "data/popup/index.html?type=extension"
  },
  "background": {
    "page": "lib/chrome/background.html"
  },
  "icons": {
    "16": "data/icons/16.png",
    "48": "data/icons/48.png",
    "128": "data/icons/128.png"
  }
}

